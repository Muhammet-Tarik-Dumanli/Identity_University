@model List<IdentityUser>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Kullanıcı Bilgisi
            </th>
            <th>
                İşlem
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    <a asp-action="Update" asp-route-id="@item.Id">Öğrenci Rolü Ata</a>
                </td>
            </tr>
        }
    </tbody>
</table>
